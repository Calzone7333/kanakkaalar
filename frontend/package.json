{
  "name": "bizzfilling-frontend",
  "version": "0.1.0",
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite preview --host --port 5173",
    "electron:dev": "concurrently -k \"cross-env IS_DEV=true npm run dev\" \"wait-on tcp:5173 && cross-env IS_DEV=true electron .\"",
    "electron:build": "electron-builder",
    "mobile:sync": "npx cap sync",
    "android:open": "npx cap open android",
    "ios:open": "npx cap open ios"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@heroicons/react": "^2.2.0",
    "@lottiefiles/dotlottie-react": "^0.17.8",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.7.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.4.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "framer-motion": "^12.23.22",
    "isotope-layout": "^3.0.6",
    "jquery": "^3.7.1",
    "lucide-react": "^0.545.0",
    "odometer": "^0.4.8",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.2",
    "react-slick": "^0.31.0",
    "recharts": "^3.3.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.19",
    "tailwindcss": "^3.4.11",
    "vite": "^5.0.0",
    "wait-on": "^9.0.3",
    "wowjs": "^1.1.3"
  },
  "description": "Bizzfilling Desktop Application",
  "author": "Bizzfilling",
  "build": {
    "appId": "com.bizzfilling.app",
    "productName": "Bizzfilling",
    "directories": {
      "output": "release"
    },
    "compression": "maximum",
    "files": [
      "electron/main.js",
      "electron/offline.html",
      "electron/icon.png",
      "!node_modules"
    ],
    "win": {
      "target": "nsis",
      "icon": "electron/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "electron/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "electron/icon.png"
    }
  },
  "dependencies": {
    "@react-oauth/google": "^0.13.4",
    "jwt-decode": "^4.0.0",
    "react-helmet-async": "^2.0.5"
  }
}
